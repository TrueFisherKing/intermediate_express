<h1>Posts Page</h1>

<ul>
    <% for(var i = 0; i < posts.length; i++){ %>
        <li> <%= posts[i].title %> - <strong><%= posts[i].author %></strong></li>
    <% } %>
</ul>

<hr>

<ul>
    <% posts.forEach( function(posts){%>
    <li> <%= posts.title %> - <strong><%= posts.author %></strong></li>
    <% }) %>
</ul>